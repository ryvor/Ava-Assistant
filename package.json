{
  "name": "ava-assistant",
  "version": "1.0.0",
  "description": "Ava will be your own fully private, local, extensible personal assistant -\r capable of doing tasks, holding conversations, understanding documents,\r and eventually living throughout your smart home -\r with a personality shaped by you.",
  "main": "index.js",
  "scripts": {
    "prune": "npx ts-prune",
    "depcheck": "npx depcheck",
    "dev:nlu": "cd nlu && rasa run --enable-api --cors \"*\" --port 5000",
    "dev:gru": "cd gru && python server.py",
    "dev:ctl": "ts-node-dev --respawn --transpile-only ava/main.ts",
    "dev:cli": "ts-node-dev --respawn --transpile-only cli/ava-cli.ts",
    "dev:web": "ts-node-dev --respawn --transpile-only web/ava-web.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryvor/Ava-Assistant.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/ryvor/Ava-Assistant/issues"
  },
  "homepage": "https://github.com/ryvor/Ava-Assistant#readme",
  "dependencies": {
    "@phosphor-icons/web": "^2.1.2",
    "bcryptjs": "^3.0.3",
    "better-sqlite3": "^12.5.0",
    "cookie-parser": "^1.4.7",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^5.0.6",
    "@types/node": "^24.10.1",
    "npm": "^11.6.4",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
